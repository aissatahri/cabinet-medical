<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="492.0" prefWidth="699.0" styleClass="mainFxmlClass" stylesheets="@../../css/bilan.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azmicro.moms.controller.bilan.BilanController">
   <children>
        <VBox alignment="TOP_LEFT" spacing="10" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
            <children>
                  <Button fx:id="btnAjouterLigneBilan" mnemonicParsing="false" onAction="#ajouterLigneBilan" text="Ajouter" />
            <AnchorPane prefHeight="355.0" prefWidth="626.0" VBox.vgrow="ALWAYS">
               <children>
                  <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <content>
                        <VBox fx:id="vboxLignesBilan" alignment="CENTER" spacing="10.0" />
                     </content>
                  </ScrollPane>
               </children>
            </AnchorPane>

                <!-- Buttons for actions -->
                <HBox prefWidth="626.0" spacing="10" VBox.vgrow="ALWAYS">
                    <Button fx:id="btnAjouteBilan" onAction="#ajouterBilan" text="Enregistrer" />
                    <Button fx:id="btnCancel" onAction="#annuler" text="Annuler" />
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
