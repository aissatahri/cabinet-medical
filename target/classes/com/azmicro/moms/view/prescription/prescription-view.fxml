<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="521.0" prefWidth="771.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azmicro.moms.controller.prescription.PrescriptionController">
    <stylesheets>
        <URL value="@../../css/prescription.css" />
    </stylesheets>
   <children>
      <VBox alignment="TOP_LEFT" spacing="10" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="718.0" spacing="10.0">
               <children>
                  <Button fx:id="btnAjouterLignePrescription" mnemonicParsing="false" onAction="#ajouterLigneImagerie" text="Ajouter" />
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </HBox>
            <AnchorPane prefHeight="389.0" prefWidth="645.0" VBox.vgrow="ALWAYS">
               <children>
                  <ScrollPane prefHeight="411.0" prefWidth="718.0" AnchorPane.bottomAnchor="-10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <content>
                        <VBox fx:id="vboxLignesPrescription" spacing="10.0" />
                     </content>
                  </ScrollPane>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </AnchorPane>
            <HBox alignment="CENTER_RIGHT" prefWidth="626.0" spacing="10" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnSavePresciption" onAction="#savePresciption" text="Enregistrer" />
                  <Button fx:id="btnCancel" onAction="#annuler" text="Annuler" />
               </children>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
