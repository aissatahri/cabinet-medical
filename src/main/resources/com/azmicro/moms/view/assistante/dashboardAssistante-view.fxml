<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" stylesheets="@../../css/dashboardassistante.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azmicro.moms.controller.assistante.DashboardAssistanteController">
   <children>
      <VBox prefHeight="474.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="17.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="33.0" prefWidth="211.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="389.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="33.0" prefWidth="181.0" text="Liste Des Patients" textFill="#581dabf7">
                           <font>
                              <Font name="Segoe UI Semibold" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="33.0" prefWidth="39.0" AnchorPane.rightAnchor="0.0">
                     <children>
                        <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="35.0" style="-fx-background-color: transparent;">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../images/shutdown.png" />
                                 </image>
                              </ImageView>
                           </graphic></Button>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="26.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="33.0" prefWidth="364.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
                     <children>
                        <StackPane prefHeight="150.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                           <children>
                              <TextField fx:id="tfKeyword" prefHeight="32.0" prefWidth="325.0" />
                              <Button mnemonicParsing="false" prefHeight="32.0" prefWidth="29.0" style="-fx-background-color: transparent;" StackPane.alignment="CENTER_RIGHT">
<!--                                 <graphic>
                                    <FontIcon iconLiteral="fas-search" />
                                 </graphic>-->
                              </Button>
                           </children>
                        </StackPane>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="57.0" prefWidth="364.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Button fx:id="btnOpenFicheNewPatient" mnemonicParsing="false" onAction="#openFicheNewPatient" text="Nouveau">
<!--                           <graphic>
                              <FontIcon iconLiteral="fas-plus" size="24" />
                           </graphic>-->
                        </Button>
                        <Button fx:id="btnOpenSalleAttente" mnemonicParsing="false" onAction="#openSalleAttente" text="Salle attente">
<!--                           <graphic>
                              <FontIcon iconLiteral=""/>
                           </graphic>-->
                        </Button>
                        <Button fx:id="btnOpenListeRendezVous" mnemonicParsing="false" onAction="#openListeRendezVous" text="les rendez-vous">
<!--                           <graphic>
                              <FontIcon iconLiteral="fas-print" size="24" />
                           </graphic>-->
                        </Button>
                        <Button fx:id="btnOpenRendezvous" mnemonicParsing="false" onAction="#openRendervous" style="-fx-background-color: transparent;">
                           <graphic>
                              <ImageView fitHeight="25.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../images/calendar.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="200.0" prefWidth="536.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <children>
                        <TableView fx:id="patientsTv" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                           <columns>
                              <TableColumn fx:id="actionClm" prefWidth="75.0" text="Actions" />
                              <TableColumn fx:id="dossierClm" prefWidth="75.0" text="N Dossier" />
                              <TableColumn fx:id="nomClm" prefWidth="75.0" text="Nom" />
                              <TableColumn fx:id="prenomClm" prefWidth="63.3333740234375" text="Prénom" />
                              <TableColumn fx:id="birthClm" prefWidth="76.66668701171875" text="Date Naissance" />
                              <TableColumn fx:id="situationFamilialClm" prefWidth="75.0" text="Situation familiale" />
                              <TableColumn fx:id="telephoneClm" prefWidth="54.66668701171875" text="Téléphone" />
                              <TableColumn fx:id="sexeClm" prefWidth="40.0" text="Sexe" />
                              <TableColumn fx:id="ageClm" prefWidth="38.0" text="Age" />
                              <TableColumn fx:id="adresseClm" prefWidth="124.0" text="Adresse" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                           
                        </TableView>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
