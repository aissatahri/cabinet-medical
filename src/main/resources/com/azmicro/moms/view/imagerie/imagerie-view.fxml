<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="453.0" prefWidth="731.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azmicro.moms.controller.imagerie.ImagerieController">
    <stylesheets>
        <URL value="@../../css/imagerie.css" />
    </stylesheets>
   <children>
      <VBox alignment="TOP_LEFT" spacing="10" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Button fx:id="btnAjouterLigneImagerie" mnemonicParsing="false" onAction="#ajouterLigneImagerie" text="Ajouter" />
            <AnchorPane prefHeight="355.0" prefWidth="626.0" VBox.vgrow="ALWAYS">
               <children>
                  <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <content>
                        <VBox fx:id="vboxLignesImagerie" spacing="10.0" />
                     </content>
                  </ScrollPane>
               </children>
            </AnchorPane>
            <HBox prefWidth="626.0" spacing="10" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnSaveImagerie" onAction="#saveBilan" text="Enregistrer" />
                  <Button fx:id="btnCancel" onAction="#annuler" text="Annuler" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
