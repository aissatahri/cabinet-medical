<AnchorPane id="AnchorPane" prefHeight="433.0" prefWidth="664.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azmicro.moms.controller.medecin.PatientController">
    <stylesheets>
        <URL value="@../../css/patient.css" />
