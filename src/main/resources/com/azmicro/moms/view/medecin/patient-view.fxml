                     <Button fx:id="btnPrint" mnemonicParsing="false" onAction="#print" text="Imprimer" />
                     <Button mnemonicParsing="false" style="-fx-background-color: transparent;" />
                  </children>
               </HBox>
            </children>
         </AnchorPane>
         <AnchorPane VBox.vgrow="ALWAYS">
